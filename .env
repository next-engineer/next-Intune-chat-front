# API Configuration
VITE_API_URL=http://localhost:8080

# WebSocket Configuration
VITE_WEBSOCKET_URL=wss://your-api-gateway-url.execute-api.region.amazonaws.com/stage

# Notion API Configuration
VITE_NOTION_API_KEY=your-notion-api-key
VITE_NOTION_DATABASE_ID=your-notion-database-id

# Development Configuration
VITE_DEV_MODE=true
VITE_DEBUG_MODE=false

# Feature Flags
VITE_ENABLE_CHAT=true
VITE_ENABLE_NOTION=true
VITE_ENABLE_PINGPONG=true

# Timeout Configuration
VITE_API_TIMEOUT=10000
VITE_WEBSOCKET_PING_INTERVAL=30000
VITE_WEBSOCKET_PONG_TIMEOUT=10000

# Build Configuration
VITE_APP_NAME=Intune Front
VITE_APP_VERSION=0.1.0
